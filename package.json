    "jest": "^26.5.3",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.20",
    "null-loader": "^4.0.0",
    "prepack-webpack-plugin": "^1.1.2",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
