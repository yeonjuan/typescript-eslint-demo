{
  "name": "typescript-eslint-demo",
  "version": "0.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "//format": "Run \"prettier\" for fixing code format",
    "format": "prettier . --write",
    "//lint": "Run \"eslint\" for linting",
    "lint": "eslint src",
    "//dev": "Run dev server",
    "dev": "webpack-dev-server --open --config webpack.dev.config",
    "//build": "Bundle files",
    "build": "ts-node scripts/rm-rf.ts ./dist && webpack --config webpack.prod.config",
    "check": "tsc --noEmit",
    "//gen": "gen",
    "gen:libs": "ts-node scripts/gen-libs.ts",
    "clear": "ts-node scripts/rm-rf.ts ./dist ",
    "//deploy": "deploy",
    "deploy": "npm run build && gh-pages -d dist",
    "//test": "test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeonjuan/typescript-eslint-demo.git"
  },
  "author": "YeonJuAn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yeonjuan/typescript-eslint-demo/issues"
  },
  "homepage": "https://github.com/yeonjuan/typescript-eslint-demo#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.0",
    "bootstrap": "^4.5.2",
    "codemirror": "^5.58.1",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "esquery": "^1.3.1",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/codemirror": "0.0.98",
    "@types/eslint": "^7.2.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.51",
    "@types/react-bootstrap": "^0.32.24",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^6.0.3",
    "css-loader": "^4.3.0",
    "dts-minify": "^0.2.0",
    "eslint-config-prettier": "^6.12.0",
    "favicons-webpack-plugin": "^4.2.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.3.1",
